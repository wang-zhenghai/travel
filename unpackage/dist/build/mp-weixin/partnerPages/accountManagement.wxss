.tn-custom-nav-bar__back{width:60%;height:100%;position:relative;display:flex;justify-content:space-evenly;align-items:center;box-sizing:border-box;background-color:rgba(0,0,0,.15);border-radius:1000rpx;border:1rpx solid hsla(0,0%,100%,.5);color:#fff;font-size:18px}.tn-custom-nav-bar__back .icon{display:block;flex:1;margin:auto;text-align:center}.tn-navbg{background:linear-gradient(-120deg,#9a5ce5,#01beff,#00f5d4,#43e97b);background-size:500% 500%;-webkit-animation:gradientBG 15s ease infinite;animation:gradientBG 15s ease infinite;position:fixed;top:0;width:100%;z-index:100}@-webkit-keyframes gradientBG{0%{background-position:0 50%}50%{background-position:100% 50%}100%{background-position:0 50%}}@keyframes gradientBG{0%{background-position:0 50%}50%{background-position:100% 50%}100%{background-position:0 50%}}